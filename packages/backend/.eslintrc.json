{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "only-warn"
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "root": true,
  "rules": {
    "semi": "warn",
    "eqeqeq": "warn"
  },
  "ignorePatterns": [
    "/dist/**"
  ]
}
